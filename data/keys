  backslash      /* REVERSE SOLIDUS */
  bracketright   /* RIGHT SQUARE BRACKET */
  asciicircum    /* CIRCUMFLEX ACCENT */
  underscore     /* LOW LINE */
  grave          /* GRAVE ACCENT */
